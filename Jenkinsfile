pipeline {
    
    agent any
    
    stages {
        
        stage('Início') {
            steps {
                bat 'echo Início'
            }
        }
        
        stage('Meio') {
            steps {
                bat 'echo Meio'
            }
        }
        
        stage('Fim') {
            steps {
                bat 'echo Fim'
            }
        }
        
    }
    
}
